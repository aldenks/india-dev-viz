#!/bin/sh

APPDIR=$(dirname "$0")
java  -Djava.library.path="$APPDIR:$APPDIR/lib" -cp "$APPDIR/lib/buttczar.jar:$APPDIR/lib/core.jar:$APPDIR/lib/jogl-all.jar:$APPDIR/lib/gluegen-rt.jar:$APPDIR/lib/jogl-all-natives-linux-i586.jar:$APPDIR/lib/gluegen-rt-natives-linux-i586.jar:$APPDIR/lib/controlP5.jar:$APPDIR/lib/gt-cql-2.7-M3.jar:$APPDIR/lib/gt-epsg-hsql-2.7-M3.jar:$APPDIR/lib/xercesImpl-2.7.1.jar:$APPDIR/lib/gt-data-2.7-M3.jar:$APPDIR/lib/commons-pool-1.5.4.jar:$APPDIR/lib/jsr-275-1.0-beta-2.jar:$APPDIR/lib/MapThing.jar:$APPDIR/lib/jdom-1.0.jar:$APPDIR/lib/vecmath-1.3.2.jar:$APPDIR/lib/gt-metadata-2.7-M3.jar:$APPDIR/lib/gt-shapefile-2.7-M3.jar:$APPDIR/lib/geoapi-2.3-M1.jar:$APPDIR/lib/hsqldb-1.8.0.7.jar:$APPDIR/lib/gt-api-2.7-M3.jar:$APPDIR/lib/gt-main-2.7-M3.jar:$APPDIR/lib/gt-referencing-2.7-M3.jar:$APPDIR/lib/jts-1.11.jar:$APPDIR/lib/geoapi-pending-2.3-M1.jar" buttczar "$@"
